developer="http://indiecomputing.com/"
url="http://ubos.net/"
maintainer=${developer}
pkgname=$(basename $(pwd))
pkgver=0.1
pkgrel=1
pkgdesc='UBOS Python utilities'
arch=('any')
url=${developer}
license=('AGPL')
depends=('python')

package() {
    cd "${startdir}/python"
    python setup.py install --root="${pkgdir}/" --optimize=1
}

